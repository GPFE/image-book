<%= form_with url: static_pages_path, method: :get do |form| %>
    <%= form.label :user_id %>
    <%= form.text_field :user_id %>
    <%= form.submit %>
<% end %>

<% if @user_photos %>
    <% @user_photos.each do |photo_url| %>
        <%= image_tag(photo_url, alt: "flickr image")%>
    <% end %>
<% end %>